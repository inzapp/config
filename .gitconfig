[core]
    editor = vi
    autocrlf = true
    whitespace = cr-at-eol
[init]
    defaultBranch = master
[pull]
    rebase = true
[diff]
    tool = vimdiff
[merge]
    tool = vimdiff
[alias]
    st = status
    graph = log --graph --abbrev-commit --decorate --format=format:'%C(cyan)%h%C(reset) - %C(green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(yellow)%d%C(reset)' --all
